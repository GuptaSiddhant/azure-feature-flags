{
  "$schema": "https://jsr.io/schema/config-file.v1.json",
  "name": "@gs/azure-feature-flags",
  "version": "1.4.0",
  "exports": {
    ".": "./src/index.ts",
    "./allocate": "./src/allocate.ts",
    "./client": "./src/client.ts",
    "./rollout": "./src/rollout.ts",
    "./service": "./src/service.ts",
    "./validate": "./src/validate.ts"
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    ".github",
    ".yarn",
    "app",
    "scripts",
    "tests",
    ".yarnrc.yml",
    "*.js",
    "*.d.ts",
    "*.html",
    "vite.config.ts",
    "yarn.lock",
    "*.json"
  ]
}