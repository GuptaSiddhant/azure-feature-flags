{
  "name": "azure-feature-flags",
  "version": "0.0.1",
  "type": "module",
  "description": "Feature flags managed via Azure Configuration",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guptasiddhant/azure-feature-flags.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guptasiddhant/azure-feature-flags/issues"
  },
  "files": [
    "cjs",
    "esm"
  ],
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    }
  },
  "scripts": {
    "build": "yarn build:esm; yarn build:cjs",
    "build:cjs": "rm -rf cjs; tsc -p tsconfig.cjs.json",
    "build:esm": "rm -rf esm; tsc",
    "test": "vitest run"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.6.0"
  },
  "packageManager": "yarn@4.1.1",
  "dependencies": {
    "@azure/app-configuration-provider": "^1.0.0-preview.4"
  }
}
